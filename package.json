{
  "name": "home",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "prettier:check": "prettier -c src",
    "format": "prettier -w src",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "dependencies": {
    "@react-three/drei": "^7.20.1",
    "@react-three/fiber": "^7.0.18",
    "autoprefixer": "^10.4.0",
    "clsx": "^1.1.1",
    "framer-motion": "^5.2.1",
    "mdx-bundler": "^7.0.0",
    "next": "^12.0.3",
    "next-themes": "0.0.15",
    "postcss": "^8.3.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-spinners": "^0.11.0",
    "three": "^0.134.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/tailwindcss": "^2.2.4",
    "@types/three": "^0.133.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.13.12",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.3",
    "eslint-config-prettier": "^8.3.0",
    "gray-matter": "^4.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "next-sitemap": "^1.6.203",
    "prettier": "^2.4.1",
    "rehype-prism-plus": "^1.1.1",
    "standard-version": "^9.3.2",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss}": [
      "prettier -w"
    ],
    "data/**/*.{md,mdx}": [
      "prettier -w"
    ]
  },
  "packageManager": "yarn@3.1.0"
}
